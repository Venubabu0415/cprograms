// Online C compiler to run C program online
#include <stdio.h>
int main() {
int n,temp,sum=0,rem;
printf("enter the number :");
scanf("%d",&n);
temp=n;
while(n>0){
    rem=n%10;
    sum=sum+(rem*rem*rem);
    n=n/10;
}
printf("the sum:%d",sum);
if(temp==sum){
    printf("\n%d is a armstrong number",temp);

}else{
    printf("%d is not a armstrong number",temp);
    
}
    return 0;
}
